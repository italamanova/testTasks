package test;

//counting brackets
public class FirstTask {
	public static int Ñi(int n,int k){
		if (k==0 || k==n)
	        return 1;
	    else
	        return Ci(n-1,k-1)+Ci(n-1,k);
	}

	public static int find(int n){
		int dn = 0;
		for(int i = 0; i < n; i++){
			System.out.println("dn  " + dn);
			dn += Ñi(n,i)*Ñi(n, n-1-i);
		}
		return dn;
	}
	public static void main(String[] args) {
		int u = 3;
		System.out.println("answer:" + find(u));
	}

}
